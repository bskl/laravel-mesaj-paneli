    "description": "The mesajpaneli.com Sms Api integration for Laravel 5.",
    "keywords": [
        "laravel",
        "laravel5",
        "mesajpaneli",
        "sms",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Barış Kartal",
            "email": "kartalbaris@gmail.com"
        },
        {
            "name": "Savaş Kartal",
            "email": "savaskartal@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": ">=5.3"
    },
    "autoload": {
        "psr-4": {
            "Bskl\\MpSms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bskl\\MpSms\\ServiceProvider"
            ],
            "aliases": {
